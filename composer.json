{
  "name": "rey-pro/yii2-addons",
  "description": "Yii2 addons",
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "addons",
    "font",
    "awesome"
  ],
  "homepage": "https://github.com/rey-pro/yii2-addons",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Kuzmenko",
      "email": "andrey.pro.rey@gmail.com",
      "homepage": "https://github.com/rey-pro/yii2-addons"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/slimscroll": "~1.3.3",
    "fortawesome/font-awesome": "~4.7",
    "onokumus/metismenu": "~v2.5.2",
    "drmonty/animate.css": "~3.5.2"
  },
  "autoload": {
    "psr-4": {
      "psr-4": {
        "rey\\yii2-addons\\": [
          "assets/",
          "widgets/"
        ]
      }
    }
  }
}
