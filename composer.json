{
  "name": "rey-pro/yii2-addons",
  "description": "Yii2 addons",
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "addons",
    "font",
    "awesome"
  ],
  "homepage": "https://github.com/rey-pro/yii2-addons",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Kuzmenko",
      "email": "andrey.pro.rey@gmail.com",
      "homepage": "https://github.com/rey-pro/yii2-addons"
    }
  ],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
