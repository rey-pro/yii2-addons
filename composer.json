  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/slimscroll": "~1.3.3",
    "bower-asset/sweetalert": "1.1.*",
    "fortawesome/font-awesome": "~4.7",
    "onokumus/metismenu": "~v2.5.2",
    "drmonty/animate.css": "~3.5.2",
    "bower-asset/flot": "~0.8.3",
    "bower-asset/icheck": "*",
    "bower-asset/bower-jquery-sparkline": "~2.1.2",
    "yiisoft/yii2-jui": "~2.0.0"
  },
  "autoload": {
    "classmap": ["src"]
  }
}
